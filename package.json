{
  "name": "organauthic-hng",
  "version": "1.0.0",
  "description": "User Authentication and Organisation",
  "main": "./src/server.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "start": "node src/server.js",
    "migrate": "knex migrate:latest",
    "migrate:latest": "knex migrate:latest --knexfile src/database/knexfile.cjs --env production",
    "seed:make": "knex seed:make --knexfile src/database/knexfile.cjs --env development",
    "seed:run": "knex seed:run --knexfile src/database/knexfile.cjs --env development",
    "migrate:rollback": "knex migrate:rollback --all --knexfile src/database/knexfile.cjs --env development",
    "knex:migrate:latest:test": "npx knex --knexfile src/database/knexfile.cjs migrate:latest --env test",
    "knex:migrate:rollback:test": "npx knex --knexfile src/database/knexfile.cjs migrate:rollback --env test",
    "knex:seed:run:test": "npx knex --knexfile src/database/knexfile.cjs seed:run --env test",
    "knex": "npx knex --knexfile src/database/knexfile.cjs",
    "knex1": "npx knex --knexfile dist/database/knexfile.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.44.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "nodemailer": "^6.9.14",
    "nyc": "^17.0.0",
    "pg": "^8.12.0",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.1",
    "chai-http": "^5.0.0",
    "jest": "^29.7.0",
    "mocha": "^10.6.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5"
  }
}
